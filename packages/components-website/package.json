{
	"name": "@wordpress/components-website",
	"version": "0.1.0",
	"description": "Documentation website for @wordpress/components",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"homepage": "https://github.com/WordPress/gutenberg/tree/master/packages/components-website/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/gutenberg.git"
	},
	"bugs": {
		"url": "https://github.com/WordPress/gutenberg/issues"
	},
	"scripts": {
		"prestart": "npm run build:docs",
		"start": "concurrently 'npm run watch:docs' 'npm run start:server' 'npm run start:app'",
		"start:app": "PORT=4700 react-scripts start",
		"start:server": "node server.js",
		"build": "npm run build:docs && npm run build:app",
		"build:app": "react-scripts build",
		"build:docs": "npm run clean:docs && npm run generate:data",
		"clean:docs": "rm -rf public/_data",
		"generate:docs": "node ./scripts/generate-docs.js",
		"generate:pkg": "node ./scripts/generate-pkg.js",
		"generate:nav": "node ./scripts/generate-nav.js",
		"generate:data": "npm run generate:docs && npm run generate:nav && npm run generate:pkg",
		"watch:docs": "node ./scripts/watch-transform-data.js"
	},
	"dependencies": {
		"@automattic/color-studio": "2.1.0",
		"@wordpress/block-serialization-default-parser": "file:../block-serialization-default-parser",
		"@wordpress/blocks": "file:../blocks",
		"@wordpress/components": "file:../components",
		"@wordpress/compose": "file:../compose",
		"@wordpress/date": "file:../date",
		"@wordpress/element": "file:../element",
		"@wordpress/i18n": "file:../i18n",
		"axios": "0.19.0",
		"classnames": "2.2.6",
		"lodash": "4.17.11",
		"marked": "0.7.0",
		"prism-react-renderer": "0.1.7",
		"remark": "11.0.1",
		"remark-parse": "7.0.1",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-helmet": "5.2.1",
		"react-live": "2.1.2",
		"react-router": "5.0.1",
		"react-router-dom": "5.0.1",
		"react-scripts": "3.0.1",
		"scroll-into-view-if-needed": "2.2.20",
		"styled-components": "4.3.2",
		"unified": "8.3.2",
		"unist-util-visit": "2.0.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}

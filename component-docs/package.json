{
	"name": "@wordpress/components-docsite",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prestart": "npm run build:docs",
		"start": "concurrently 'npm run watch:docs' 'npm run start:server' 'npm run start:app'",
		"start:app": "PORT=4700 react-scripts start",
		"start:server": "node server.js",
		"build": "npm run build:docs && npm run build:app",
		"build:app": "react-scripts build",
		"build:docs": "npm run clean:docs && npm run generate:data",
		"clean:docs": "rm -rf public/_data",
		"generate:docs": "node ./scripts/generate-docs.js",
		"generate:pkg": "node ./scripts/generate-pkg.js",
		"generate:nav": "node ./scripts/generate-nav.js",
		"generate:data": "npm run generate:docs && npm run generate:nav && npm run generate:pkg",
		"watch:docs": "node ./scripts/watch-transform-data.js"
	},
	"dependencies": {
		"@automattic/color-studio": "2.1.0",
		"@wordpress/block-serialization-default-parser": "^3.2.0",
		"@wordpress/blocks": "^6.2.0",
		"@wordpress/components": "^7.3.1",
		"@wordpress/compose": "3.5.0",
		"@wordpress/date": "3.4.0",
		"@wordpress/element": "^2.3.0",
		"@wordpress/i18n": "3.6.0",
		"axios": "0.19.0",
		"classnames": "2.2.6",
		"copy-to-clipboard": "3.2.0",
		"fuse.js": "3.4.5",
		"lodash": "4.17.11",
		"marked": "0.7.0",
		"prism-react-renderer": "0.1.7",
		"query-string": "6.8.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "5.2.1",
		"react-live": "2.1.2",
		"react-router": "5.0.1",
		"react-router-dom": "5.0.1",
		"react-scripts": "3.0.1",
		"reselect": "4.0.0",
		"scroll-into-view-if-needed": "2.2.20",
		"styled-components": "4.3.2",
		"use-debounce": "3.0.0"
	},
	"devDependencies": {
		"chokidar": "3.0.2",
		"concurrently": "4.1.1",
		"express": "4.17.1",
		"fast-glob": "3.0.4",
		"mkdirp": "0.5.1",
		"remark": "11.0.1",
		"remark-parse": "7.0.1",
		"unified": "8.3.2",
		"unist-util-visit": "2.0.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
